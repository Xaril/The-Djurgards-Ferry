          <argument>
            <kind>1</kind>
            <string>///Move the buoy
hspd = global.water_speed;
x += hspd;

//Make sure buoys aren't too close
var size = ds_list_size(global.buoys);
for(var i = 0; i &lt; size; i++)
{
    if(global.buoys[| i] != id)
    {
        other_x = global.buoys[| i].x;
        if(abs(other_x - x) &lt; 200)
        {
            if(other_x - x &lt; 0)
            {
                x = other_x + 200;
            }
            else
            {
                global.buoys[| i].x = x + 200;
            }
        }
    }   
}

//Remove buoys outside the game screen
if(x &lt; -64)
{
    pos = ds_list_find_index(global.buoys, id);
    ds_list_delete(global.buoys, pos);
    instance_destroy();
}
</string>
